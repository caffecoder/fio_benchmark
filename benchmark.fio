[global]
directory=/tmp/fio
size=100M
direct=1
runtime=5
numjobs=5
group_reporting

[rand-write-4k]
rw=randwrite
bs=4k

[rand-write-64k]
rw=randwrite
bs=64k

[rand-write-512k]
rw=randwrite
bs=512k

[seq-write-4k]
rw=write
bs=4k

[seq-write-64k]
rw=write
bs=64k

[seq-write-512k]
rw=write
bs=512k

[rand-read-4k]
rw=randread
bs=4k

[rand-read-64k]
rw=randread
bs=64k

[rand-read-512k]
rw=randread
bs=512k

[seq-read-4k]
rw=read
bs=4k

[seq-read-64k]
rw=read
bs=64k

[seq-read-512k]
rw=read
bs=512k
